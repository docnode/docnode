{
  "name": "docnode",
  "version": "0.1.1",
  "license": "SEE LICENSE IN LICENSE.md",
  "description": "A type-safe, fast, ID-based Operational Transformation (OT) framework.",
  "type": "module",
  "homepage": "https://docnode.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/docnode/docnode.git",
    "directory": "packages/docnode"
  },
  "scripts": {
    "dev": "tsc -p tsconfig.build.json --watch",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf .next node_modules .turbo dist tsconfig.build.tsbuildinfo || rmdir /s /q .next node_modules .turbo dist tsconfig.build.tsbuildinfo",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/exports/index.d.ts",
      "default": "./dist/src/exports/index.js"
    }
  },
  "dependencies": {
    "ulid": "^3.0.1"
  }
}
