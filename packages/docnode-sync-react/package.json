{
  "name": "@docnode/sync-react",
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "scripts": {
    "dev": "tsc -p tsconfig.build.json --watch",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf .next node_modules .turbo dist tsconfig.build.tsbuildinfo || rmdir /s /q .next node_modules .turbo dist tsconfig.build.tsbuildinfo",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6"
  },
  "dependencies": {
    "@docnode/sync": "workspace:*",
    "docnode": "workspace:*",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "exports": {
    ".": {
      "types": "./dist/src/exports.d.ts",
      "default": "./dist/src/exports.js"
    }
  }
}
